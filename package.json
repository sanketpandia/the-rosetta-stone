{
  "name": "the-rosetta-stone",
  "displayName": "the-rosetta-stone",
  "description": "To initialise internationalization language files. Picks text based on user selection and adds it to the language file",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "the-rosetta-stone.selectLanguage",
      "title": "Rosetta Stone: Select Repository Language"
    },{
      "command": "the-rosetta-stone.selectFormat",
      "title": "Rosetta Stone: Select Translation Format"
    },
  {
    "command": "the-rosetta-stone.addTranslation",
    "title": "Rosetta Stone: Add Translation to file"
  }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
